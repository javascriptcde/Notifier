{
  "expo": {
    "name": "Notifier",
    "slug": "Notifier",
    "version": "2026.1.3",
    "orientation": "portrait",
    "icon": "./assets/images/AppIcon.png",
    "scheme": "notifier",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thomasl.notifier",
      "usesAppleSignIn": false,
      "usesIcloudStorage": false,
      "icon": "./assets/images/AppIcon.png",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "remote-notification"
        ],
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses your location to send alerts when you enter specific zones.",
        "NSLocationWhenInUseUsageDescription": "This app uses your location to send alerts when you enter specific zones.",
        "NSLocationAlwaysUsageDescription": "This app uses your location to send alerts even when the app is in the background.",
        "CFBundleIcons": {
          "CFBundlePrimaryIcon": {
            "CFBundleIconFiles": [
              "AppIconLight",
              "AppIconLight"
            ]
          },
          "CFBundleAlternateIcons": {
            "DarkIcon": {
              "CFBundleIconFiles": [
                "AppIconDark",
                "AppIconDark"
              ]
            }
          }
        },
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSExceptionDomains": {
            "tiles.stadiamaps.com": {
              "NSIncludesSubdomains": true,
              "NSTemporaryExceptionAllowsInsecureHTTPLoads": true,
              "NSTemporaryExceptionMinimumTLSVersion": "TLSv1.2"
            },
            "demotiles.maplibre.org": {
              "NSIncludesSubdomains": true,
              "NSTemporaryExceptionAllowsInsecureHTTPLoads": true,
              "NSTemporaryExceptionMinimumTLSVersion": "TLSv1.2"
            },
            "api.maptiler.com": {
              "NSIncludesSubdomains": true,
              "NSTemporaryExceptionAllowsInsecureHTTPLoads": false,
              "NSTemporaryExceptionMinimumTLSVersion": "TLSv1.2"
            }
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/androidforegroundicon.png",
        "backgroundImage": "./assets/images/androidbackgroundicon.png",
        "monochromeImage": "./assets/images/androidmonochromeicon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "googleServicesFile": "./google-services.json",
      "package": "com.thomasl.notifier",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/AppIcon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-notifications",
      "@maplibre/maplibre-react-native",
      "expo-web-browser",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "mapProvider": "maptiler",
      "maptilerKey": "P6xL3GTk8oM1rxbEtoly",
      "stadiaKey": "b0866975-3a25-4498-ad04-f5c184d0c370",
      "eas": {
        "projectId": "e456df79-8331-4b9d-a6fa-7f6c6730e6d2"
      }
    },
    "owner": "tling"
  }
}
